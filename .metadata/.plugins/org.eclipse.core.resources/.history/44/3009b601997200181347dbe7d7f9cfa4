
public class Person {
	public static void main(String[]args){
		Person taro = new.Person("山田太郎", 20);
		System.out.println(taro.name);
	}

	private String name = null;
	private int age = 0;

	public String getName(){
		return this.name;
	}
	public void setName(String name){
		this.name = name;
	}

}
